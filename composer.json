{
    "name": "kytoonlabs/laravel-helm",
    "description": "Laravel wrapper for HELM v3",
    "version": "1.0.0",
    "type": "library",
    "require": {
        "illuminate/support": "*",
        "php": "^8.2|^8.3",
        "symfony/process": "^6.2"
    },
    "require-dev": {
        "orchestra/testbench": "^8.2",
        "phpunit/phpunit": "^9.6"

    },
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Kytoonlabs\\LaravelHelm\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Enrique Ponce de Leon",
            "email": "enrique@kytoonlabs.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Kytoonlabs\\LaravelHelm\\HelmServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Kytoonlabs\\LaravelHelm\\Tests\\": "tests/"
        }
    }
}
